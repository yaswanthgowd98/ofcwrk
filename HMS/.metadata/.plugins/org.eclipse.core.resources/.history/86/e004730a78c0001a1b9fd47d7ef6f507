  <script>
  $('#followupdate').datepicker({
		format : 'dd/mm/yyyy'
	});
  function  myFunction() {
  if(document.getElementById("followup").checked) {
	    document.getElementById('followupdate').disabled = false;
	    $('#followupdate').prop('required','required');
	}
  else
	  {
	  document.getElementById('followupdate').disabled = true;
	  }
  }
  </script>
  
  
  
