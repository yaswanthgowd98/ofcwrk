package com.example.demo.dao;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;

import com.example.demo.model.Admin;

public interface Arep extends CrudRepository<Admin, Integer>{

	public boolean existsByApassword(String apassword);
	public boolean existsByAa1(String aa1);
	public boolean existsByAa2(String aa2);
	public boolean existsByAa3(String aa3);
	

	
}
